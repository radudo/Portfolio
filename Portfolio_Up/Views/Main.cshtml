@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Main>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = null;
}
<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="author" content="Dobos Radu 2018">


    <title></title>

    <!--CSS-->
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/Content/uikit.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/2.9.6/jquery.fullpage.css" rel="stylesheet" />

</head>
<body>
    <a class="uk-overlay-panel uk-position-top-right uk-icon w " uk-sticky="top" uk-icon="icon: menu; ratio: 2" href="#menu-modal" uk-toggle>

    </a>

    <div id="menu-modal" class="uk-modal-full" uk-modal>
        <div class="uk-modal-dialog">

            <button class="uk-modal-close-default w" type="button" uk-icon="icon: close; ratio: 2"></button>
            <div class=" uk-position-left logo"><span class="highlight">R</span>adu Dobos</div>
            <div class="uk-flex-middle ">
                <div class="uk-background gradient-menu" uk-height-viewport>
                    <div id="nav-primary">
                        <ul class="uk-nav uk-nav-primary uk-nav-center uk-margin-auto-vertical uk-position-medium uk-position-center" id="myMenu">


                            <li data-menuanchor="firstPage" class="uk-active" id="first" ><a href="#">Intro</a></li>

                            <li data-menuanchor="secondPage" id="second"><a href="#">About</a></li>
                            <li data-menuanchor="thirdPage" id="third"><a href="#">Projects</a></li>

  
                        </ul>

                    </div>
                </div>
                <ul class="uk-iconnav uk-position-large uk-position-bottom-right">
                    <li><a href="#" uk-icon="icon: linkedin"></a></li>
                    <li><a href="#" uk-icon="icon: github"></a></li>
                    <li><a href="#" uk-icon="icon: instagram"></a></li>
                </ul>
            </div>

        </div>
    </div>


    <div id="fullpage">
        <div class="section" data-anchor="section-1" id="section-1">





            <div class="uk-overlay-panel uk-position-center uk-light " style=" z-index:999!important;">
                <div class=" uk-overlay-panel ">
                    <h1 uk-parallax="target: #section-1; y: 200; easing: 0" >Hello, I'm <span class="highlight">Radu Dobos</span> .</h1>
                    <h2>I am a Web Developer. <span class="uk-h2" id="msg"></span></h2>
                </div>
            </div>


            <span id="movedown" class="uk-overlay-panel uk-position-bottom-center uk-icon-button" uk-icon="icon: chevron-down; ratio: 3">

            </span>
            <div class="uk-position-cover uk-animation-kenburns uk-animation-reverse uk-transform-origin-center-left" id="b">
                <img src="~/Media/bg.jpeg" alt="" uk-cover uk-background-contain>
            </div>


        </div>
        <div class="section " data-anchor="section-2">

        </div>
        <div class="section" data-anchor="section-3">Some section</div>

    </div>


    <!--Javascript Files-->
    <script src="~/Scripts/uikit.js"></script>
    <script src="~/Scripts/uikit-icons.js"></script>
    <script src="~/Scripts/jquery-3.0.0.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/2.9.6/jquery.fullpage.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/2.9.6/vendors/jquery.easings.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/2.9.6/vendors/scrolloverflow.js"></script>
    <script src="~/Scripts/initializer.js"></script>

   
</body>
</html>                               