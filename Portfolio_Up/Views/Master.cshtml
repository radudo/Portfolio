@inherits UmbracoViewPage<Master>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
                /**/

                Layout = null;
}

<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta name="description" content="@Model.MetaDescription">
    <meta name="keywords" content="@Model.MetaKeywords">
    <meta name="author" content="@Model.Author @DateTime.Now.ToString("yyyy")">
    <link rel="shortcut icon" href="~/Media/favicon.ico" type="image/x-icon">



    <title>@Umbraco.Field("pageName")</title>

    <!--CSS-->
    <link href="~/Content/device-mockups.min.css" rel="stylesheet" />
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/Content/uikit.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/2.9.6/jquery.fullpage.css" rel="stylesheet" />
    <link href="~/Content/coding.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-grid.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
</head>
<body>

    <a class="uk-overlay-panel uk-position-top-right uk-icon w " uk-sticky="top" uk-icon="icon: menu; ratio: 2" href="#menu-modal" uk-toggle>

    </a>
    @Html.Partial("modal")

    <div id="fullpage">
        <div class="section" data-anchor="section-1" id="section-1">
            <div class="uk-visible@m">@Html.Partial("section1")</div>
            <div class="uk-hidden@m">@Html.Partial("section1-tablet")</div>

        </div>
        <div class="section " data-anchor="section-2">
            <div class="uk-visible@m">
                @Html.Partial("section2")
            </div>
            <div class="uk-hidden@m">
                @Html.Partial("section2-tablet - Copy")
            </div>
        </div>

        <div class="section" data-anchor="section-3">
            @Html.Partial("section3")
        </div>
    </div>



    <script src="~/Scripts/uikit.js"></script>
    <script src="~/Scripts/uikit-icons.js"></script>
    <script src="~/Scripts/jquery-3.0.0.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/2.9.6/jquery.fullpage.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/2.9.6/vendors/jquery.easings.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/2.9.6/vendors/scrolloverflow.js"></script>
    <script src="~/Scripts/scrolloverflow.js"></script>

    <script src="~/Scripts/initializer.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/vivus/0.4.3/vivus.js"></script>



</body>

</html>